msgstr "{noServices}"

msgid "Show/hide informations about cookie storage"
msgstr "Toon/verberg informatie over cookie opslag"

msgid "Cookies management panel"
msgstr "Cookies beheer paneel"

msgid "Cookie detail for"
msgstr "Cookie detail voor"

msgid "on our site"
msgstr "op onze site"

msgid "(new window)"
msgstr "(nieuw venster)"

